                    "200": {
                        "description": "the verification document as base64",
                        "content": {
                            "application/json": {
                                "schema":{
                                    "$ref": "#/components/schemas/CustomerAddressVerificationGetResponse"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ]
            }
        }
