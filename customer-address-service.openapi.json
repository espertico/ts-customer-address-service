        "schemas": {
            "CustomerAddressesGetResponse": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "addressId": {
                            "type": "string",
                            "description": "the unique Id of the address",
                            "example": "879b57a7-843a-41d8-991b-7970c8431594"
                        },
                        "address": {
                            "type": "object",
                            "description": "the address of a customer",
                            "allOf": [
                                {
                                    "$ref": "#/components/schemas/Address"
                                }
                            ]
                        }
                    }
                }
            },
            "CustomerAddressPostRequest": {
                "type": "object",
                "properties": {
                    "address": {
                        "type": "object",
                        "description": "the address of a customer",
                        "allOf": [
                            {
                                "$ref": "#/components/schemas/Address"
                            }
                        ]
                    }
                },
                "required": ["address"]
            },
            "CustomerAddressPostResponse": {
                "type": "object",
                "properties": {
                    "addressId": {
                        "type": "string",
                        "description": "a unique identifier of the newly persisted address",
                        "example": "3fbb832a-28b9-4f4d-8e2a-3bf65c50c2e6"
                    }
                },
                "required": ["addressId"]
            },
            "CustomerAddressVerificationRequest": {
                "type": "object",
                "allOf": [
                    {
                        "$ref": "#/components/schemas/CustomerAddressVerificationDocument"
                    }
                ]
            },
            "CustomerAddressVerificationGetResponse": {
                "type": "object",
                "allOf": [
                    {
                        "$ref": "#/components/schemas/CustomerAddressVerificationDocument"
                    }
                ]
            },
            "CustomerAddressVerificationDocument": {
                "type": "object",
                "properties": {
                    "base64": {
                        "type": "string",
                        "description": "verification document as base64 encoded string",
                        "example": "YWxidXM="
                    }
                },
                "required": [
                    "base64"
                ]
            },
            "CustomerAddressNotFound": {
                "type": "object",
                "properties": {
                    "customerId": {
                        "type": "string",
                        "description": "An ID of the customer"
                    },
                    "addressId": {
                        "type": "string",
                        "description": "An ID of the address"
                    },
                    "message": {
                        "type": "string",
                        "description": "An error message"
                    }
                }
            },
            "Address": {
                "type": "object",
                "properties": {
                    "street": {
                        "type": "string",
                        "description": "The street of the address.",
                        "example": "Goethestra√üe"
                    },
                    "houseNumber": {
                        "type": "string",
                        "description": "The house number of the address including extra characters like in \"29a\".",
                        "example": "29a"
                    },
                    "optionalLine": {
                        "type": "string",
                        "description": "This contains any additional information regarding location of the address at the specified housenumber",
                        "example": "Block C, staircase 5, LEFT"
                    },
                    "zipCode": {
                        "type": "string",
                        "description": "The zip code of the address.",
                        "example": "10162"
                    },
                    "city": {
                        "type": "string",
                        "description": "The city of the address.",
                        "example": "Berlin"
                    },
                    "countryCode": {
                        "title": "enum",
                        "description": "The country code of the country where the address is located.",
                        "type": "string",
                        "example": "DE",
                        "allOf": [
                            {
                                "$ref": "#/components/schemas/CountryCode"
                            }
                        ]
                    },
                    "residentSinceDate": {
                        "type": "string",
                        "description": "The date since when a certain person is living at the address. This should only be filled if the resident is a person.",
                        "example": "2000-11-20",
                        "pattern": "^(-?(?:[1-9][0-9]*)?[0-9]{4})-(1[0-2]|0[1-9])-(3[01]|0[1-9]|[12][0-9])(T(2[0-3]|[01][0-9]):([0-5][0-9]):([0-5][0-9])(\\.[0-9]+)?(Z)?)?$"
                    },
                    "isMainAddress": {
                        "type": "boolean",
                        "description": "A flag that can be used to mark an address as the main one when multiple addresses are available e.g. in customer data.",
                        "example": true
                    }
                },
                "required": ["street", "houseNumber", "zipCode", "city", "countryCode"]
            },
