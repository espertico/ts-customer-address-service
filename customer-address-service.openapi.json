    "paths": {
        "/customers/{customerId}/addresses": {
            "post": {
                "tags": ["CustomerAddressManagement"],
                "operationId": "createCustomerAddress",
                "summary": "creates new customer",
                "description": "creates a new customer with a specified address",
                "parameters": [
                    {
                        "name": "customerId",
                        "in": "path",
                        "description": "Id of the specified customer",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/CustomerAddressPostRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "201": {
                        "description": "Returns the stored customer address and its identifier",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CustomerAddressPostResponse"
                                }
                            }
                        }
                    }
                },
                "x-codegen-request-body-name": "CustomerAddressPostRequest",
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ]
            },
            "get": {
                "tags": ["CustomerAddressManagement"],
                "operationId": "getCustomerAddresses",
                "summary": "Get Customer Addresses",
                "description": "Gets the Adresseses for the specified customer",
                "parameters": [
                    {
                        "name": "customerId",
                        "in": "path",
                        "description": "Id of the specified customer",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "the customer address of the specified customer",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CustomerAddressesGetResponse"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Resource not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "additionalProperties": true
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ]
            }
        },
        "/customers/{customerId}/addresses/{addressId}": {
            "delete": {
                "tags": ["CustomerAddressManagement"],
                "operationId": "deleteCustomerAddresses",
                "summary": "deletes Customer Addresses",
                "description": "deletes an address for the specified customer",
                "parameters": [
                    {
                        "name": "customerId",
                        "in": "path",
                        "description": "Id of the specified customer",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "addressId",
                        "in": "path",
                        "description": "Id of the specified address",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "resource deleted successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "additionalProperties": true
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "resource not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "additionalProperties": true
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ]
            }
        },
        "/customers/{customerId}/addresses/{addressId}/verification": {
            "post": {
                "tags": ["CustomerAddressVerification"],
                "operationId": "createVerificationDocument",
                "summary": "uploads verification document",
                "description": "uploads a verification document and saves it in an S3 bucket.",
                "parameters": [
                    {
                        "name": "customerId",
                        "in": "path",
                        "description": "Id of the specified customer",
                        "required": true,
                        "x-obfuscate": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "addressId",
                        "in": "path",
                        "description": "Id of the specified customer",
                        "required": true,
                        "x-obfuscate": false,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/CustomerAddressVerificationRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "201": {
                        "description": "The customer id with a conformation that the document has been saved",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CustomerAddressPostResponse"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Could not find the customer or the address for that specific customer",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CustomerAddressNotFound"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                         "ApiKeyAuth": [] 
                    }
                ]
            },
            "get": {
                "tags": ["CustomerAddressVerification"],
                "operationId": "getVerificationDocument",
                "summary": "gets verification Document",
                "description": "checks if customer ",
                "parameters": [
                    {
                        "name": "customerId",
                        "in": "path",
                        "description": "Id of the specified customer",
                        "required": true,
                        "x-obfuscate": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "addressId",
                        "in": "path",
                        "description": "Id of the specified customer",
                        "required": true,
                        "x-obfuscate": false,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
